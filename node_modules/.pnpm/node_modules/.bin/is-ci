#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lois/Documents/Github/vite-example/node_modules/.pnpm/is-ci@4.1.0/node_modules/is-ci/node_modules:/Users/lois/Documents/Github/vite-example/node_modules/.pnpm/is-ci@4.1.0/node_modules:/Users/lois/Documents/Github/vite-example/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lois/Documents/Github/vite-example/node_modules/.pnpm/is-ci@4.1.0/node_modules/is-ci/node_modules:/Users/lois/Documents/Github/vite-example/node_modules/.pnpm/is-ci@4.1.0/node_modules:/Users/lois/Documents/Github/vite-example/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../is-ci/bin.js" "$@"
else
  exec node  "$basedir/../is-ci/bin.js" "$@"
fi
